    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:deployment": "export REACT_APP_GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) && craco build",
    "test": "craco test --transformIgnorePatterns 'node_modules/(?!@polkadot)/'",
    "test:ci": "CI=true craco test --transformIgnorePatterns 'node_modules/(?!@polkadot)/'",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache --transformIgnorePatterns 'node_modules/(?!@polkadot)/'",
    "test:e2e-ci": "CI=true npx playwright test /e2e-tests/tests",
    "test:e2e-local": "CI=false npx playwright test /e2e-tests/tests",
    "eject": "craco eject",
    "prettier": "prettier **/** --check --ignore-path .gitignore",
    "lint": "eslint src --max-warnings=0",
    "commit": "git-cz",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --out-file src/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --ignore **/*.d.ts",
    "translations:compile": "formatjs compile src/lang/en.json --out-file src/compiled-lang/en.json",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch src/**/*.graphql",
    "prepare": "husky install"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint-config-prettier"
    ],
    "ignorePatterns": "src/generated/**/*.tsx",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "*"
        ],
        "rules": {
          "react/style-prop-object": [
            1,
            {
              "allow": [
                "FormattedNumber"
              ]
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "prettier --ignore-unknown --ignore-path .gitignore --write"
    ],
    "src/**/!(graphql).{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "babel": {
    "plugins": [
      [
        "formatjs",
        {
          "idInterpolationPattern": "[sha512:contenthash:base64:6]",
          "ast": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@formatjs/cli": "^4.3.1",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/core": "^2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "@playwright/test": "^1.17.1",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.4.4",
    "@storybook/node-logger": "^6.3.11",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.11",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/lodash": "^4.14.177",
    "babel-plugin-formatjs": "^10.3.9",
    "bignumber.js": "^9.0.1",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "css-loader": "5.2.6",
    "cz-conventional-changelog": "^3.2.0",
    "node-sass": "^6.0.1",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.42.1",
    "sass-loader": "10.1.1",
    "storybook-react-router-dom": "^6.1.21",
    "style-loader": "2.0.0",
    "wait-for-expect": "^3.0.2",
    "xunit-viewer": "^7.1.5"
  },
  "resolutions": {
    "playwright": "1.17.0-rc1"
  }
}
